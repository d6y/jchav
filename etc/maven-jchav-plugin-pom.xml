<?xml version="1.0"?><project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.googlecode.jchav</groupId>
  <artifactId>maven-jchav-plugin</artifactId>
  <packaging>maven-plugin</packaging>
  <name>Maven JChav Plugin</name>
  <version>1.1.0</version>
  <url>http://jchav.blogspot.com</url>
  <build>
  		<directory>../dist</directory>
  		<outputDirectory>../dist/classes</outputDirectory>
		<sourceDirectory>../contrib</sourceDirectory>
  </build>
  <repositories>
    <repository>
      <id>jchav</id>
      <url>../releases/maven</url>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>org.apache.maven</groupId>
      <artifactId>maven-plugin-api</artifactId>
      <version>2.0</version>
    </dependency>
    <dependency>
      <groupId>jfree</groupId>
      <artifactId>jcommon</artifactId>
      <version>1.0.5</version>
    </dependency>
    <dependency>
      <groupId>jfree</groupId>
      <artifactId>jfreechart</artifactId>
      <version>1.0.2</version>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
      <version>1.2.13</version>
    </dependency>
    <dependency>
      <groupId>org.googlecode.jchav</groupId>
      <artifactId>jchav</artifactId>
      <version>1.1.0</version>
    </dependency>
  </dependencies>
</project>
