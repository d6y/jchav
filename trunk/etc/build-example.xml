<?xml version="1.0" encoding="UTF-8"?>

<project name="jchav-example" default="default" basedir=".">

	<target name="default">
	
		 <taskdef name="jchav" classname="com.googlecode.jchav.ant.JChavTask">
			<classpath>
				<fileset dir="../libs/">
					<include name="**/*.jar"/>
				</fileset>
				<fileset dir="..">
					<include name="jchav.jar"/>
				</fileset>
			</classpath>
		 </taskdef>
		
		<jchav srcdir="../testdata/digwalk" destdir=""/>
	</target>
	
</project>

